{
  "name": "raftstack",
  "version": "1.0.0",
  "description": "CLI tool for setting up Git hooks, commit conventions, and GitHub integration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "raftstack": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "cli",
    "git-hooks",
    "husky",
    "commitlint",
    "lint-staged",
    "developer-experience"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "commander": "^12.0.0",
    "execa": "^8.0.0",
    "picocolors": "^1.0.0"
  }
}
